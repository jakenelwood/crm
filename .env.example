# RingCentral API credentials
RINGCENTRAL_CLIENT_ID=your_client_id
RINGCENTRAL_CLIENT_SECRET=your_client_secret
RINGCENTRAL_SERVER=https://platform.ringcentral.com
RINGCENTRAL_USERNAME=your_username@example.com
RINGCENTRAL_EXTENSION=101
RINGCENTRAL_PASSWORD=your_password
RINGCENTRAL_FROM_NUMBER=+15551234567
RINGCENTRAL_OAUTH_SCOPES=ReadCallLog ReadMessages RingOut SMS

# RingCentral WebRTC (client-side)
NEXT_PUBLIC_RINGCENTRAL_CLIENT_ID=your_client_id
NEXT_PUBLIC_RINGCENTRAL_SERVER=https://platform.ringcentral.com
NEXT_PUBLIC_RINGCENTRAL_FROM_NUMBER=+15551234567
NEXT_PUBLIC_RINGCENTRAL_DOMAIN=platform.ringcentral.com

# Next.js public URL for OAuth redirect
# Local development URLs (uncomment these for local development)
NEXT_PUBLIC_APP_URL=http://localhost:3000
REDIRECT_URI=http://localhost:3000/oauth-callback

# Production URLs (uncomment these for production)
# NEXT_PUBLIC_APP_URL=https://your-production-url.com
# REDIRECT_URI=https://your-production-url.com/oauth-callback

# Supabase credentials
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
